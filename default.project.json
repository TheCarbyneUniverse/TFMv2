{
  "name": "TFMv2",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,

      "TFMv2": {
        "$path": "src/TFMv2.lua"
      }

    },

    "StarterGui": {
      "$className": "StarterGui",
      "$ignoreUnknownInstances": true,

      "Test": {
        "$path": "src/TestScript.client.lua"
      },

      "TFMv2 Showcase": {
        "$className": "ScreenGui",

        "$properties": {
          "IgnoreGuiInset": true
        },
        
        "Background": {
          "$className": "Frame",

          "$properties": {

            "BackgroundColor3": [0.4, 0.4, 0.4],
            "BorderSizePixel": 0,
            "Size": [1, 0, 1, 0]

          },

          "BasicCountdown": {
            "$className": "TextLabel",
  
            "$properties": {
  
              "AnchorPoint": [0.5, 0.5],
              "Position": [0.5, 0, 0.5, -150],
              "Size": [0.875, 0, 0, 100],
              "BackgroundTransparency": 1,
              "TextColor3": [1, 1, 1],
              "TextSize": 25,
              "TextScaled": true,
              "Font": "Code",
              "TextXAlignment": "Center",
              "TextYAlignment": "Center",
              "Text": "Basic Countdown"
  
            }
  
          },
  
          "Stopwatch": {
            "$className": "TextLabel",
  
            "$properties": {
  
              "AnchorPoint": [0.5, 0.5],
              "Position": [0.5, 0, 0.5, 0],
              "Size": [0.875, 0, 0, 100],
              "BackgroundTransparency": 1,
              "TextColor3": [1, 1, 1],
              "TextSize": 25,
              "TextScaled": true,
              "Font": "Code",
              "TextXAlignment": "Center",
              "TextYAlignment": "Center",
              "Text": "Stopwatch"
  
            }
  
          },
  
          "Sentence": {
            "$className": "TextLabel",
  
            "$properties": {
  
              "AnchorPoint": [0.5, 0.5],
              "Position": [0.5, 0, 0.5, 150],
              "Size": [0.875, 0, 0, 100],
              "BackgroundTransparency": 1,
              "TextColor3": [1, 1, 1],
              "TextSize": 25,
              "TextScaled": true,
              "Font": "Code",
              "TextXAlignment": "Center",
              "TextYAlignment": "Center",
              "Text": "Sentence"
  
            }
  
          }

        }

      }

    }
  }
}